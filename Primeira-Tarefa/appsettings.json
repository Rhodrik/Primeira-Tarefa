{
  "Serilog": {
    "MinimunLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": { "path": "Logs/log.txt" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "postgres": "Host=localhost; Username=postgres; Password=log; Database=postgres; Search Path=first_task"
  }
}
